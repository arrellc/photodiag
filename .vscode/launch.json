{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "palm-alvra",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "pythonPath": "${config:python.pythonPath}",
            "module": "bokeh",
            "cwd": "${workspaceFolder}",
            "args": ["serve", "${workspaceFolder}/apps/palm", "--args", "alvra"],
            "console": "none",
            "env": {},
            "debugOptions": ["RedirectOutput"]
        },
        {
            "name": "palm-bernina",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "pythonPath": "${config:python.pythonPath}",
            "module": "bokeh",
            "cwd": "${workspaceFolder}",
            "args": ["serve", "${workspaceFolder}/apps/palm", "--args", "bernina"],
            "console": "none",
            "env": {},
            "debugOptions": ["RedirectOutput"]
        },
    ]
}
